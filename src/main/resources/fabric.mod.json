{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Serena's QoLmod",
  "description": "A collection of a few QoL (Quality of Life) features that I wish were in minecraft.",
  "authors": [
    "Sunskimmer82#6969"
  ],
  "contact": {
    "sources": "https://github.com/Sunskimmer822/Serenas-QOLMod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.serenas.qolmod.QOLMod"
    ],
    "client": [
      "com.serenas.qolmod.QOLModClient"
    ]
  },
  "mixins": [
    "qolmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "1.19.3",
    "java": ">=17",
    "fabric-key-binding-api-v1": "*"
  },

  "accesswidener": "qolmod.accesswidener"
}
